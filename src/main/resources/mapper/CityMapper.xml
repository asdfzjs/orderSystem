<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.orderSystem.dao.CityMapper" >
 
  
  
  <select id="getCityCode" resultType="String" parameterType="String" >
    select
    code from t_city where name = #{name, jdbcType=VARCHAR} 
  </select>
</mapper>